      <section class="bg-primary" id="one">
        <div class="container">
          <div class="row">
            <section class="bg-primary" id="one">
              <div class="container">
                <div class="row">
                  <div class="col-lg-12  text-center">
                    <h2 class="margin-top-0 text-primary">Earthquake report</h2>
                    <div class="row vertical-offset-100">
                      <div class="col-md-12">
                        <div class="panel panel-default">
                          <div class="panel-body">
                            <form accept-charset="UTF-8" role="form"  method="post">
                              
                             
                              
                             <table id="example" class="display" cellspacing="0" width="100%">
                              <thead>
                                <tr>
                                  <th>State Name</th>
                                  <th>Longitude</th>
                                  <th>Latitude</th>
                                  <th>Depth</th>
                                  <th>Magnitude</th>
                                  <th> Time of Occurance</th>
                                  
                                </tr>
                              </thead>
                              <tfoot>
                                <tr>
                                 <th>State Name</th>
                                 <th>Longitude</th>
                                 <th>Latitude</th>
                                 <th>Depth</th>
                                 <th>Magnitude</th>
                                 <th> Time of Occurance</th>
                               </tr>
                             </tfoot>
                             <tbody>
                              <% for (var i in  data  ) { %>
                              <tr>
                                <td><%= data[i].State_name %></td>
                                <td><%= data[i].Longitude_state %></td>
                                <td><%= data[i].Latitude_state %></td>
                                <td><%= data[i].Depth_state %></td>
                                <td><%= data[i].Magnitude_state%></td>
                                <td><%= data[i].TimeOfOccurance_state%></td>
                              </tr>    
                              <% } %>
                            </tbody>
                          </table>
                          
                          
                          
                        </form>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              
              
              
              
              <!--scripts loaded here from cdn for performance -->
              <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
